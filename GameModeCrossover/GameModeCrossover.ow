rule("Doomfist - lock player into 1v1")
{
	event
	{
		Player Dealt Damage;
		All;
		Doomfist;
	}

	conditions
	{
		Hero Of(Event Player) == Hero(Doomfist);
		Is Alive(Event Player) == True;
		Player Variable(Event Player, doom1v1Target) == Null;
	}

	actions
	{
		Set Player Variable(Event Player, doom1v1Target, Victim);
		Teleport(Event Player, Value In Array(Global Variable(DOOM_ARENA_POS), 0));
		Teleport(Player Variable(Event Player, doom1v1Target), Value In Array(Global Variable(DOOM_ARENA_POS), 1));
		Start Camera(Event Player, Add(Divide(Add(Position Of(Event Player), Position Of(Player Variable(Event Player, doom1v1Target))),
			2), Global Variable(DOOM_CAMERA_OFFSET)), Divide(Add(Position Of(Event Player), Position Of(Player Variable(Event Player,
			doom1v1Target))), 2), 100);
		Start Camera(Player Variable(Event Player, doom1v1Target), Add(Divide(Add(Position Of(Event Player), Position Of(Player Variable(
			Event Player, doom1v1Target))), 2), Global Variable(DOOM_CAMERA_OFFSET)), Divide(Add(Position Of(Event Player), Position Of(
			Player Variable(Event Player, doom1v1Target))), 2), 100);
		Start Forcing Throttle(Event Player, 0, 1, 0, 1, 0, 0);
		Start Forcing Throttle(Player Variable(Event Player, doom1v1Target), 0, 1, 0, 1, 0, 0);
		Heal(Event Player, Event Player, 1000);
		Heal(Player Variable(Event Player, doom1v1Target), Event Player, 1000);
		Disallow Button(Event Player, Jump);
		Disallow Button(Player Variable(Event Player, doom1v1Target), Jump);
	}
}

