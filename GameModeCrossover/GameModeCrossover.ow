		Count Of(Player Variable(Event Player, junkPropIds)) < 11;
	}

	actions
	{
		Set Player Variable(Event Player, pos, Add(Position Of(Event Player), Vector(Random Real(Multiply(Global Variable(
			JUNK_PROP_RADIUS), -1.000), Global Variable(JUNK_PROP_RADIUS)), 0, Random Real(Multiply(Global Variable(JUNK_PROP_RADIUS),
			-1.000), Global Variable(JUNK_PROP_RADIUS)))));
		Modify Player Variable(Event Player, junkPropPosList, Append To Array, Player Variable(Event Player, pos));
		Create Effect(All Players(All Teams), Sphere, Green, Player Variable(Event Player, pos), Global Variable(JUNK_PROP_RADIUS), None);
		Modify Player Variable(Event Player, junkPropIds, Append To Array, Last Created Entity);
		Wait(0.016, Ignore Condition);
		Loop If Condition Is True;
	}
}

rule("Junkrat - Move Prop Effects")
{
	event
	{
		Ongoing - Each Player;
		All;
		Junkrat;
	}

	conditions
	{
		Hero Of(Event Player) == Hero(Junkrat);
		Is Alive(Event Player) == True;
		Horizontal Speed Of(Event Player) > 0;
	}

	actions
	{
		Modify Player Variable(Event Player, index, Add, 1);
		Skip If(Compare(Player Variable(Event Player, index), <, Count Of(Player Variable(Event Player, junkPropIds))), 1);
		Set Player Variable(Event Player, index, 0);
		Skip If(Compare(Distance Between(Position Of(Event Player), Value In Array(Player Variable(Event Player, junkPropPosList),
			Player Variable(Event Player, index))), <, Multiply(Global Variable(JUNK_PROP_RADIUS), 2.500)), 5);
		Set Player Variable(Event Player, pos, Add(Position Of(Event Player), Vector(Random Real(Multiply(Global Variable(
			JUNK_PROP_RADIUS), -1.000), Global Variable(JUNK_PROP_RADIUS)), 0, Random Real(Multiply(Global Variable(JUNK_PROP_RADIUS),
			-1.000), Global Variable(JUNK_PROP_RADIUS)))));
		Set Player Variable At Index(Event Player, junkPropPosList, Player Variable(Event Player, index), Player Variable(Event Player,
			pos));
		Destroy Effect(Value In Array(Player Variable(Event Player, junkPropIds), Player Variable(Event Player, index)));
		Create Effect(All Players(All Teams), Sphere, Green, Player Variable(Event Player, pos), Global Variable(JUNK_PROP_RADIUS), None);
		Set Player Variable At Index(Event Player, junkPropIds, Player Variable(Event Player, index), Last Created Entity);
		Wait(0.160, Ignore Condition);
		Loop If Condition Is True;
	}
}

